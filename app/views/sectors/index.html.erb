<p style="color: green"><%= notice %></p>

<% content_for :title, "Sectors" %>

<h1>Sectors</h1>

<div id="sectors">
  <% @sectors.each do |sector| %>
    <%= render sector %>
    <%= sector.name %>
    <%= sector.unit.name %>

    <p>
      <%= link_to "Show this sector", sector %>
    </p>
  <% end %>
</div>

<%= link_to "New sector", new_sector_path %>
